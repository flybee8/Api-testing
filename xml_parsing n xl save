import osimport sysimport ms.versionms.version.addpkg('openpyxl', '2.3.3')ms.version.addpkg('numpy', '1.11.1')ms.version.addpkg('jdcal', '1.0')ms.version.addpkg('et_xmlfile', '1.0.1')ms.version.addpkg('pandas', '0.18.0')ms.version.addpkg('dateutil','2.5.2')ms.version.addpkg('pytz','2016.6.1')ms.version.addpkg('six','1.9.0')import xml.etree.ElementTree as ETimport openpyxl as pyxlfrom openpyxl import Workbooktree = ET.ElementTree(file='1003859.xml')root = tree.getroot()wb=Workbook()wb.as_template = Truews=wb.activews.title = 'Recommind_Data'iter_ = tree.iter()for elem in iter_:    if '}' in elem.tag:        elem.tag=elem.tag.split('}',1)[1]    #NME=elem.attrib.get('name')    #print(elem.tag,elem.text)    for row in zip ({elem.tag},{elem.text}):        ws.append(row)dims = {}for row in ws.rows:    for cell in row:        if cell.value:            dims[cell.column] = max((dims.get(cell.column, 0), len(cell.value)))    for col, value in dims.items():        ws.column_dimensions[col].width = valuewb.save('Recommind_1003859.xlsx')
